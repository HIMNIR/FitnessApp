<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="Fitnessapp.Pages.ListViewClass"
             Title="ListViewClass"
             BackgroundColor="{StaticResource Blackest}">

    <ListView>
       
        <ListView.ItemTemplate>
            <DataTemplate>
                <ViewCell>
                    <StackLayout Background="{StaticResource light-black}" >
                        <Label  Margin="5" Text="{Binding .}" Style="{StaticResource CommonLabelStyle}"  HorizontalOptions="Center"></Label>
                    </StackLayout>
                </ViewCell>
            </DataTemplate>
        </ListView.ItemTemplate>
        
    </ListView>
    
    
</ContentPage>